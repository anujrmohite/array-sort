#include<stdio.h>
#include<stdlib.h>

struct array
{
int A[20];
int size;
int length;
};

int arraySort(struct array *arr)
{
    int i,j,temp;
    for ( i = 0; i < arr->length; i++)
    {
        for ( j = i+1; j < arr->length; j++)
        {
            if (arr->A[i]>arr->A[j])
            {
                temp=arr->A[i];
                arr->A[i]=arr->A[j];
                arr->A[j]=temp;      
            }
            
        }
        
    }
    
}


void arrayDisplay(struct array arr)
{
    int i;
    for ( i = 0; i < arr.length; i++)
    {
        printf(" %d ",arr.A[i]);
    }
    
}
int main()
{
    struct array arr={{3,46,77,7,1},20,5};
    arraySort(&arr);
    arrayDisplay(arr);
    return 0;
}
